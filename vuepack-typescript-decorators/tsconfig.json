{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "pretty": true,
    "lib": [
      "es5",
      "dom",
      "es2015.promise"
    ],
    "types": [
      "vue-typescript-import-dts"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}