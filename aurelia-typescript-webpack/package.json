{
  "name": "aurelia-typescript-webpack",
  "productName": "Aurelia TypeScript and Webpack",
  "description": "Testing the Aurelia framework. Based on aurelia-skeleton-navigation-webpack.",
  "version": "0.0.0",
  "main": "dist/index.html",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development yarn run webpack -- --progress --profile",
    "build:prod": "cross-env NODE_ENV=production yarn run webpack -- --progress --profile",
    "build": "yarn run build:dev",
    "clean:dist": "rimraf dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && rimraf node_modules test/coverage dist",
    "e2e:live": "yarn run e2e:start -- --elementExplorer",
    "e2e:start-when-ready": "wait-on --timeout 20000 http-get://localhost:19876/index.html && yarn run e2e:start",
    "e2e:start": "cross-env ./node_modules/.bin/protractor test/protractor.conf.js",
    "e2e": "concurrently --kill-others \"yarn run e2e:start-when-ready\" \"cross-env WEBPACK_PORT=19876 npm start\"",
    "prebuild:dev": "yarn run clean:dist",
    "prebuild:prod": "yarn run clean:dist",
    "preclean:install": "yarn run clean",
    "preclean:start": "yarn run clean",
    "pree2e": "yarn run webdriver:update -- --standalone",
    "server:dev:hmr": "yarn run server:dev -- --hot",
    "server:dev": "cross-env NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --progress --profile --watch",
    "server:prod": "http-server dist --cors",
    "server": "yarn run server:dev",
    "start": "yarn run server:dev",
    "test": "cross-env NODE_ENV=test ./node_modules/karma/bin/karma start test/karma.conf.js",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
    "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
    "webpack-dev-server": "cross-env ./node_modules/.bin/webpack-dev-server",
    "webpack": "cross-env ./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aurelia/skeleton-navigation.git"
  },
  "keywords": [
    "aurelia",
    "typescript",
    "webpack"
  ],
  "license": "CC0-1.0",
  "author": "Jan Aagaard <jan@aagaard.net> (http://cpnhgn.dk/)",
  "dependencies": {
    "aurelia-event-aggregator": "1.0.0",
    "aurelia-fetch-client": "1.0.1",
    "aurelia-framework": "1.0.7",
    "aurelia-history-browser": "1.0.0",
    "aurelia-loader-webpack": "1.0.3",
    "aurelia-logging-console": "1.0.0",
    "aurelia-pal-browser": "1.0.0",
    "aurelia-polyfills": "1.1.1",
    "aurelia-route-recognizer": "1.1.0",
    "aurelia-router": "1.0.7",
    "aurelia-templating-binding": "1.0.0",
    "aurelia-templating-resources": "1.1.1",
    "aurelia-templating-router": "1.0.0",
    "bootstrap": "4.0.0-alpha.5",
    "font-awesome": "4.7.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.37",
    "@types/whatwg-fetch": "0.0.32",
    "aurelia-bootstrapper-webpack": "1.0.0",
    "aurelia-tools": "1.0.0",
    "aurelia-webpack-plugin": "1.1.0",
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "http-server": "0.9.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-remap-istanbul": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "3.11.2",
    "protractor": "4.0.10",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "stylelint": "7.5.0",
    "stylelint-config-standard": "14.0.0",
    "ts-node": "1.7.0",
    "typescript": "2.0.8",
    "url-loader": "0.5.7",
    "wait-on": "1.5.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  }
}
