    "react-dom": "15.4.0",
    "react-redux": "4.4.6",
    "redux": "3.6.0",
    "redux-typescript-actions": "1.1.0",
    "serve-static": "1.11.1",
    "todomvc-app-css": "2.0.6",
    "yargs": "6.4.0"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
