    "build:rollup": "rollup -c",
    "fix": "tslint --fix src/**/*.ts src/**/*.tsx",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "start": "yarn install && webpack-dev-server"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "mobx": "3.1.0",
    "mobx-react": "4.1.0",
    "mobx-react-devtools": "4.2.11",
    "mobx-react-router": "3.1.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2"
  },
  "devDependencies": {
    "@types/react": "15.0.11",
    "@types/react-dom": "0.14.23",
    "@types/react-router": "3.0.4",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.0",
    "rimraf": "2.6.0",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-typescript": "0.8.1",
    "sass-loader": "6.0.1",
    "style-loader": "0.13.1",
    "ts-loader": "2.0.0",
    "tslint": "4.4.2",
    "tslint-eslint-rules": "3.4.0",
    "tslint-react": "2.4.0",
    "typescript": "2.1.6",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "0.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  }
