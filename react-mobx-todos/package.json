    "@types/react-router": "2.0.41",
    "babili-webpack-plugin": "0.0.7",
    "cross-env": "3.1.3",
    "html-webpack-plugin": "2.24.1",
    "rimraf": "2.5.4",
    "rollup": "0.36.4",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-typescript": "0.8.1",
    "ts-loader": "1.3.1",
    "tslint": "4.0.2",
