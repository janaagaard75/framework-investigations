    "react-router-dom": "4.1.1"
  },
  "devDependencies": {
    "@types/react": "15.0.23",
    "@types/react-dom": "15.5.0",
    "@types/react-router": "4.0.5",
    "@types/react-router-dom": "4.0.4",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
