{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "files": [
        "js/constants.ts",
        "js/interfaces.d.ts",
        "js/todoModel.ts",
        "js/utils.ts",
        "js/app.tsx",
        "js/footer.tsx",
        "js/todoItem.tsx"
    ]
}
