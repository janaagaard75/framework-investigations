{
    "compilerOptions": {
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": false,
        "isolatedModules": false,
        "jsx": "react",
        "module": "amd",
        "moduleResolution": "node",
        "newLine": "LF",
        "noImplicitAny": false,
        "noLib": false,
        "outDir": "js",
        "preserveConstEnums": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "files": [
        "src/constants.ts",
        "src/interfaces.d.ts",
        "src/KeyCode.ts",
        "src/TodoApp.tsx",
        "src/TodoFooter.tsx",
        "src/TodoItem.tsx",
        "src/TodoModel.ts",
        "src/Utils.ts"
    ]
}
