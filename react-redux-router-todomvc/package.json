{
  "name": "redux-react-router-todomvc",
  "version": "1.0.0",
  "description": "An implementation of TodoMVC using React, Redux, React-Router, and more!",
  "repository": "tough-griff/redux-react-router-todomvc",
  "author": "Griffin Yourick",
  "license": "MIT",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "build": "yarn run clean && yarn run build:webpack",
    "check": "yarn run lint && yarn run test",
    "clean": "rimraf assets coverage",
    "lint": "eslint --ext=.js,.jsx js/ test/ *.js",
    "start": "babel-node server.js",
    "test:cov": "NODE_ENV=test babel-node $(yarn bin)/isparta cover $(yarn bin)/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test yarn run test -- --watch --growl",
    "test": "NODE_ENV=test mocha --recursive --compilers=js:babel-register --require=./test/helpers/jsdom.js"
  },
  "engines": {
    "node": "5.x"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "camel-case": "3.0.0",
    "classnames": "2.2.5",
    "constant-mirror": "1.1.1",
    "fetch-check-http-status": "2.0.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "react": "15.4.1",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "todomvc-app-css": "2.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "css-loader": "0.26.0",
    "eslint": "3.11.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "expect.js": "0.3.1",
    "express": "4.14.0",
    "fetch-mock": "5.5.0",
    "isparta": "4.0.0",
    "jsdom": "9.8.3",
    "json-server": "0.9.1",
    "mocha": "3.2.0",
    "morgan": "1.7.0",
    "node-sass": "3.13.0",
    "react-addons-test-utils": "15.4.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.3",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  }
}
