{
  "extends": "tslint:latest",
  "rules": {
    "align": [
      false
    ],
    "array-type": [
      true,
      "generic"
    ],
    "arrow-parens": false,
    "eofline": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  },
  "jsRules": {
    "eofline": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      false
    ]
  }
}